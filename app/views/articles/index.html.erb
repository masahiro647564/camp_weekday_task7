<div class="container ">
  <div class="row">
    <div class="col-md-12">
      <h1><% @articles.each do |article| %></h1>
      <%= article.created_at.strftime('%b%d,%Y') %>
        <p>
          <%= link_to article.title, article %>
        </p>
        
      <% end %>

      <%= link_to "新規論文投稿", new_article_path %>
    </div>
  </div>
</div>